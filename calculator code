nummer1 = float(input("ange första nummret"))
typ = input("välj mellan + - * /")
nummer2 = float(input("ange andra nummret"))


if(typ == "+"):
  svar = nummer1 + nummer2
if(typ == "-"):
  svar = nummer1 - nummer2
if(typ == "*"):
  svar = nummer1 * nummer2
if(typ == "/"):
  svar = nummer1 / nummer2
  
  
print(str(nummer1) + " " + typ + " " + str(nummer2) + " = " + str(svar))
